public static void main(String [] args) throws Exception
	{
		flashAuction helloThere = new flashAuction(123,0); //edge case where auction time is 0 
    flashAuction helloThere2 = new flashAuction(123,4); 
    
		
		Wallet person1 = new Wallet("abc",50); 
		Wallet person2 = new Wallet("abd",80); 
		Wallet person3 = new Wallet("abe", 100); 
		helloThere2.addBid(10, person1);

		helloThere2.addBid(50, person2); 

		helloThere2.addBid(51, person3); // where bid is not 5% greater than last bid
		
		helloThere2.addBid(1000, person3);// where person3 does not have enough money
		
		
		
		
	}
